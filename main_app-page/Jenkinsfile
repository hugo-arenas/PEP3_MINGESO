pipeline {
    agent any

    stages {
        stage('Build') {
            steps {
                bat 'npm install -g npm@7.5.0'
            }
        }
        stage('Test') {
            steps {
				dir('main_app-page'){
					bat 'npm install modules'
					bat 'npm start'
				}
            }
        }
        stage('Deploy') {
            steps {
				echo 'hola...'
            }
        }
    }
}